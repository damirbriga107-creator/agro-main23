# Production Environment Configuration
NODE_ENV=production
PORT=8080

# Security
JWT_SECRET=your-production-jwt-secret-key-change-this-in-production
JWT_REFRESH_SECRET=your-production-refresh-secret-key-change-this-in-production

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres123@localhost:5432/daorsagro
DB_HOST=localhost
DB_PORT=5432
DB_NAME=daorsagro
DB_USERNAME=postgres
DB_PASSWORD=postgres123

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# CORS Configuration
CORS_ORIGIN=http://localhost:8080,https://yourdomain.com

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# External APIs
WEATHER_API_KEY=your-weather-api-key
USDA_API_KEY=your-usda-api-key

# File Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Monitoring
SENTRY_DSN=your-sentry-dsn